<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfilRadnika">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="170dp"
        android:layout_height="170dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="32dp"
        android:layout_marginRight="32dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/imeiprezime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ime i prezime"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="64dp"
        android:layout_marginLeft="32dp"/>

    <TextView
        android:id="@+id/datumrodjenja"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Datum rodjenja"
        app:layout_constraintTop_toTopOf="@id/imeiprezime"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="64dp"
        android:layout_marginLeft="32dp"/>

    <TextView
        android:id="@+id/mestorodjenja"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mesto rodjenja"
        app:layout_constraintTop_toTopOf="@id/datumrodjenja"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="64dp"
        android:layout_marginLeft="32dp"/>

    <TextView
        android:id="@+id/ocenaradnesposobnosti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ocena radne sposobnosti"
        app:layout_constraintTop_toTopOf="@id/mestorodjenja"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="64dp"
        android:layout_marginLeft="32dp"/>

    <TextView
        android:id="@+id/radnoiskustvo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Radno iskustvo"
        app:layout_constraintTop_toTopOf="@id/ocenaradnesposobnosti"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="64dp"
        android:layout_marginLeft="32dp"/>

    <TextView
        android:id="@+id/dodatnesposobnosti"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dodatne sposobnosti"
        app:layout_constraintTop_toTopOf="@id/radnoiskustvo"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="64dp"
        android:layout_marginLeft="32dp"/>

    <TextView
        android:id="@+id/cena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cena"
        app:layout_constraintTop_toTopOf="@id/dodatnesposobnosti"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="64dp"
        android:layout_marginLeft="32dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>